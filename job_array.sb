#!/bin/bash
#SBATCH --job-name=revGOL_array
#SBATCH --output=result_%a.txt
#SBATCH --time=2:00:00
#SBATCH --mem=2G
#SBATCH --array=1-50
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=1

module load OpenMPI

SEED=$SLURM_ARRAY_TASK_ID

echo "Running seed $SEED"
./revGOL cmse2.txt $SEED > result_$SEED.txt
